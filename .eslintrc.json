{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "extends": [
        "plugin:@ngrx/recommended-requiring-type-checking"
      ]
    },
    {
      "files": [
        "*.ts"
      ],
      "extends": [
        "plugin:ngrx/recommended"
      ]
    }
  ]
}
